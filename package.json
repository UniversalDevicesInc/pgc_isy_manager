  "name": "pgc_isy_manager",
  "version": "1.0.4",
  "description": "Polyglot v2 Swarm ISY Manager",
  "main": "isy_manager.js",
  "scripts": {
    "start:dev": "AWS_PROFILE=pgc-test LOCAL=true nodemon isy_manager.js",
    "dockerbuild": "docker build --rm -t einstein42/$npm_package_name:version-$npm_package_version .",
    "dockerpush": "docker login && docker push einstein42/$npm_package_name:version-$npm_package_version",
    "patchandrelease": "npm version patch --force",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UniversalDevicesInc/pgc_isy_manager.git"
  },
  "dependencies": {
    "aws-sdk": "^2.387.0",
    "dotenv": "6.2.0",
    "querystring": "0.2.0",
    "request": "2.87.0",
    "request-promise-native": "1.0.5",
    "xml2json-light": "1.0.6"
  }
}
